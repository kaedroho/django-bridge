{
  "name": "@django-bridge/vue",
  "version": "0.6.0-rc.1",
  "description": "The simple way to build Django applications with modern Vue frontends",
  "repository": "https://github.com/django-bridge/django-bridge",
  "license": "BSD-3-Clause",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "/dist",
    "/src"
  ],
  "scripts": {
    "build": "tsdown",
    "check-types": "vue-tsc --noEmit"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vue/tsconfig": "^0.8.1",
    "tsdown": "^0.14.2",
    "typescript": "^5.9.2",
    "unplugin-vue": "^7.0.1",
    "vue-tsc": "^3.0.6"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  }
}
